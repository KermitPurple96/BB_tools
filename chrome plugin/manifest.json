{
  "manifest_version": 3,
  "name": "Form Analyzer",
  "version": "1.0",
  "description": "Analiza formularios y posibles vulnerabilidades en la p√°gina",
  "permissions": ["scripting", "tabs", "storage"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ]
}
